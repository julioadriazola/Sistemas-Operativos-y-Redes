class Inodo():
	def __init__(self,filesize,accesstimes,pointers):
		self.filesize = filesize
		self.accesstimes = accesstimes
		self.pointers = pointers


for n in range(0,20):
	fh = open( "ram/"+str(n), "a")

def system_celu(texto, archivo):
	while(len(texto) > 0):
		if(len(texto) > 499):
			bloque = texto[:499]
		else:
			bloque = texto
		for x in range(1,799):
			f = open(x,"a")
			archivo = f.read(1)
				if(not archivo):
					f.write(bloque)

					x = 900

